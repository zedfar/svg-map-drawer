{
  "name": "svg-map-drawer",
  "private": false,
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "version": "1.0.22",
  "type": "module",
  "main": "dist/svg-map.umd.js",
  "module": "dist/svg-map.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/svg-map.es.js",
      "require": "./dist/svg-map.umd.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "dist-app"
  ],
  "description": "Interactive SVG map component for React with zoom, tooltip, and region coloring",
  "keywords": [
    "svg",
    "map",
    "react",
    "component",
    "visualization",
    "indonesia",
    "react-component",
    "data-visualization"
  ],
  "license": "MIT",
  "author": {
    "name": "defazr",
    "email": "ulfar.far@gmail.com"
  },
  "homepage": "https://svg-map-drawer.netlify.app/",
  "repository": {
    "type": "git",
    "url": "https://github.com/zedfar/svg-map-drawer.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:lib": "vite build --config vite.lib.config.ts",
    "build:app": "vite build --config vite.app.config.ts --sourcemap false && cp dist-app/svg-map.bundle.umd.js dist-app/svg-map.bundle.min.js",
    "build:types": "tsc --project tsconfig.lib.json --emitDeclarationOnly --declaration --outDir dist",
    "build:all": "npm run clean && npm run build:lib && npm run build:app && npm run build:types",
    "clean": "rimraf dist dist-app",
    "lint": "eslint .",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit -p tsconfig.app.json",
    "prepublishOnly": "npm run build:all"
  },
  "engines": {
    "node": ">=18"
  },
  "peerDependencies": {
    "react": ">=18.0.0 <20.0.0",
    "react-dom": ">=18.0.0 <20.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "lucide-react": "^0.344.0",
    "postcss": "^8.4.35",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2",
    "vite-plugin-static-copy": "^3.1.3"
  }
}
